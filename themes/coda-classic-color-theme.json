	"tokenColors": [
		{
			"settings": {
				"foreground": "#000000ff",
				"background": "#ffffffff"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#009000"
			}
		},
		{
			"name": "String",
			"scope": "string, support.type.property-name.json, source.json",
			"settings": {
				"fontStyle": "",
				"foreground": "#1A1AA6"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#0F20F6"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "",
				"foreground": "#916319"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"fontStyle": "",
				"foreground": "#916319"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#916319"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "",
				"foreground": "#CA0063"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#CA0063"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#CC4C07"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#CC4C07"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#053369"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "",
				"foreground": "#053369"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag, constant.language.php, constant.language.json",
			"settings": {
				"fontStyle": "",
				"foreground": "#881181"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#881181"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#7520AF"
			}
		},
		{
			"name": "Class Variable",
			"scope": "variable.other, variable.js, punctuation.separator.variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#996633"
			}
		},
		{
			"name": "Language Constant",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Keyword Operator",
			"scope": "keyword.operator, declaration.function.operator, meta.preprocessor.c.include",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Meta Brace",
			"scope": "punctuation.section.embedded -(source string source punctuation.section.embedded), meta.brace.erb.html",
			"settings": {
				"foreground": "#F02A1D"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#EB291C"
			}
		},
		{
			"name": "Function Call",
			"scope": "meta.function-call",
			"settings": {
				"fontStyle": "",
				"foreground": "#163369"
			}
		},
		{
			"name": "Keyword Control",
			"scope": "keyword.control",
			"settings": {
				"fontStyle": "",
				"foreground": "#AA2063"
			}
		},
		{
			"name": "SublimeLinter Warning",
			"scope": "sublimelinter.mark.warning",
			"settings": {
				"foreground": "#EDBA00"
			}
		},
		{
			"name": "SublimeLinter Gutter Mark",
			"scope": "sublimelinter.gutter-mark",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "SublimeLinter Error",
			"scope": "sublimelinter.mark.error",
			"settings": {
				"foreground": "#DA2000"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316bcd"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#cd3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
}